    <validationRules>
        <fullName>xcdhcm__Age_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(FLOOR((TODAY() - xcdhcm__DOB__c)/365.25)&lt;15,
   FLOOR((TODAY() - xcdhcm__DOB__c)/365.25)&gt;85)</errorConditionFormula>
        <errorMessage>Age must be between 15 and 85</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__BACS_requires_account_details</fullName>
        <active>true</active>
        <description>Validation to ensure if Payment method is BACS then account details are added</description>
        <errorConditionFormula>and(
  ispickval(xcdhcm__Payment_method__c, &quot;BACS&quot;),
  or(
    isblank(xcdhcm__P_Sort_code__c),  
    isblank(xcdhcm__Account_number__c),
    isblank(xcdhcm__Account_name__c)))</errorConditionFormula>
        <errorMessage>You have selected a payment method of BACS and have not supplied bank details. Please complete the account name, sort code, and account number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Change_In_Name</fullName>
        <active>true</active>
        <description>change name only from the change name button under personal tab of employee.</description>
        <errorConditionFormula>OR
(
	IF
	(
		AND
                (
                  CONTAINS( xcdhcm__Employee_Details__r.xcdhcm__User__r.Profile.Name , &apos;HRMS&apos;), 
                  OR
		  (
			ISCHANGED( xcdhcm__Legal_name__c ),
			ISCHANGED( xcdhcm__Known_name__c ),
			ISCHANGED( xcdhcm__Middle_name__c),
			ISCHANGED( xcdhcm__Previous_name__c)
		   )
                ),
		IF
		(
			xcdhcm__Employee_Details__r.xcdhcm__User__r.xcdhcm__Allow_Name_Change__c, 
			false,
			true
		),
		false
	),
	IF
	(
		AND
                (
                  CONTAINS( xcdhcm__Employee_Details__r.xcdhcm__User__r.Profile.Name , &apos;HRMS&apos;), 
                  ISCHANGED(Name)
                ),
		IF
		(
			AND
			(
				Name ==  xcdhcm__First_name__c + &apos; &apos;+ xcdhcm__Last_name__c,
				xcdhcm__Employee_Details__r.xcdhcm__User__r.xcdhcm__Allow_Name_Change__c 
			),
			false,
			true
		),
		false
	)
)</errorConditionFormula>
        <errorMessage>Change names for HR users using the Change name button from personal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__EmpDetail_Sort_Code_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(not(ISBLANK( xcdhcm__P_Sort_code__c )),NOT(REGEX( xcdhcm__P_Sort_code__c ,&quot;[0-9]{2}-[0-9]{2}-[0-9]{2}&quot;)),FALSE)</errorConditionFormula>
        <errorDisplayField>xcdhcm__P_Sort_code__c</errorDisplayField>
        <errorMessage>Sort Code should be of the format 99-99-99</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_Admin_sets_NI_and_bank_details</fullName>
        <active>true</active>
        <description>Validation rule is to allow only admin to change the following fields for Payroll clients 
1)Title 
2)Name 
3)Gender 
4)NI number</description>
        <errorConditionFormula>IF(AND(NOT(CONTAINS($Profile.Name, &apos;Admin&apos;)),OR(xcdhcm__Employee_Details__r.xcdhcm__Company__r.xcdhcm__Net_payroll__c, xcdhcm__Employee_Details__r.xcdhcm__Company__r.xcdhcm__Parent_Company__r.xcdhcm__Net_payroll__c)), OR(ISCHANGED(xcdhcm__Title__c),ISCHANGED(Name),ISCHANGED(xcdhcm__Gender__c),ISCHANGED(xcdhcm__National_insurance_number__c)), false)</errorConditionFormula>
        <errorMessage>Please contact the payroll administrator to change your address details.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_Clients_Need_Tax_Code_and_Basis</fullName>
        <active>true</active>
        <description>Check that if a payroll client is inputting tax code then they also input a value for tax basis</description>
        <errorConditionFormula>and(
not(isblank( xcdhcm__Employee_Details__r.xcdhcm__Job_Role__r.xcdhcm__Companies__r.xcdhcm__Employer__c)),
not(isblank( xcdhcm__Tax_code__c)),
not(ispickval( xcdhcm__Tax_basis__c, &quot;Cumulative&quot;)),
not(ispickval( xcdhcm__Tax_basis__c, &quot;Week1/Month1&quot;)))</errorConditionFormula>
        <errorDisplayField>xcdhcm__Tax_code__c</errorDisplayField>
        <errorMessage>Please ensure that when you input Tax code you also add a Tax basis (i.e., Cumulative or Week1/Month1)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Starter_A_must_be_cumulative</fullName>
        <active>true</active>
        <errorConditionFormula>and(
text( xcdhcm__Starter_declaration__c ) = &quot;A&quot;,
text( xcdhcm__Tax_basis__c ) &lt;&gt; &quot;Cumulative&quot;,
isblank(priorvalue (xcdhcm__Tax_basis__c)))</errorConditionFormula>
        <errorMessage>You have entered a Starter declaration of A.  Please set the tax basis as Cumulative</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Starter_B_no_YTD_must_be_W1M1</fullName>
        <active>true</active>
        <errorConditionFormula>and(
text( xcdhcm__Starter_declaration__c ) = &quot;B&quot;,
text( xcdhcm__Tax_basis__c ) = &quot;Cumulative&quot;,
isblank(priorvalue (xcdhcm__Tax_basis__c)),
isblank( xcdhcm__Taxable_pay_to_date__c ))</errorConditionFormula>
        <errorMessage>You have entered a Starter declaration of B but have not provided any P45 YTD earnings.  Please set the tax basis as Week 1 Month 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Starter_C_BR_or_0T_and_W1M1</fullName>
        <active>true</active>
        <description>Where option C is selected for the Starter declaration:
- tax code must be either BR or 0T
- tax basis must be W1M1</description>
        <errorConditionFormula>and(
text( xcdhcm__Starter_declaration__c ) = &quot;C&quot;,
isblank(priorvalue(xcdhcm__Tax_basis__c)),
or(
  text( xcdhcm__Tax_basis__c ) = &quot;Cumulative&quot;,
   and( xcdhcm__Tax_code__c &lt;&gt; &quot;BR&quot;,
       xcdhcm__Tax_code__c &lt;&gt; &quot;0T&quot;))
)</errorConditionFormula>
        <errorMessage>You have entered a Starter declaration of C.
If the employee has another job tax code should be BR.
If you entered C because the employee did not provide a declaration, tax code should be 0T
Please set the tax basis as Week 1 Month 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Student_loan_must_have_a_payment_plan</fullName>
        <active>true</active>
        <description>Since 2018 if an employee has a student loan this must also have a payment plan</description>
        <errorConditionFormula>or(
and(xcdhcm__Student_loan__c ,ISBLANK(text( xcdhcm__Student_loan_plan__c )),ISBLANK(text(xcdhcm__Postgraduate_loan_plan__c))),
and(not(xcdhcm__Student_loan__c) ,or(not(ISBLANK(text( xcdhcm__Student_loan_plan__c ))),not(ISBLANK(text(xcdhcm__Postgraduate_loan_plan__c)))))
)</errorConditionFormula>
        <errorMessage>If the employee has a student and / or postgraduate loan the loan must also have a payment plan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Tax_code_not_OT</fullName>
        <active>true</active>
        <description>Clients often enter OT rather than 0T - this prevents data entry error</description>
        <errorConditionFormula>or(xcdhcm__Tax_code__c = &quot;OT&quot;, xcdhcm__Tax_code__c = &quot;oT&quot;)</errorConditionFormula>
        <errorMessage>You have entered OT as the tax code.  Please check - did you intend to enter 0T</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
