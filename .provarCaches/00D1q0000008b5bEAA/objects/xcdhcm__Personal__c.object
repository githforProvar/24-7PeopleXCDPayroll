        <formula>TEXT(
IF(MONTH(TODAY())= MONTH(xcdhcm__DOB__c),
IF(DAY(TODAY())&lt; DAY(xcdhcm__DOB__c),
YEAR(TODAY())- YEAR(xcdhcm__DOB__c) - 1,
YEAR(TODAY())- YEAR(xcdhcm__DOB__c)),
IF(MONTH(TODAY())&lt; MONTH(xcdhcm__DOB__c),
YEAR(TODAY())- YEAR(xcdhcm__DOB__c) - 1,
YEAR(TODAY())- YEAR(xcdhcm__DOB__c))
) 
) &amp; &quot; year(s) &amp; &quot; &amp;
TEXT(
IF(YEAR(TODAY())= YEAR(xcdhcm__DOB__c),
IF(MONTH(TODAY())= MONTH(xcdhcm__DOB__c),
0, 
IF(DAY(TODAY())&lt; DAY(xcdhcm__DOB__c),
MONTH(TODAY())- MONTH(xcdhcm__DOB__c) - 1,
MONTH(TODAY())- MONTH(xcdhcm__DOB__c))
),
IF(MONTH(TODAY())= MONTH(xcdhcm__DOB__c),
IF(DAY(TODAY())&lt; DAY(xcdhcm__DOB__c),
11,
0),
IF(MONTH(TODAY())&gt; MONTH(xcdhcm__DOB__c),
IF(DAY(TODAY())&lt; DAY(xcdhcm__DOB__c),
MONTH(TODAY())- MONTH(xcdhcm__DOB__c) - 1,
MONTH(TODAY())- MONTH(xcdhcm__DOB__c)),
IF(DAY(TODAY())&lt; DAY(xcdhcm__DOB__c),
11 - (MONTH(xcdhcm__DOB__c) - MONTH(TODAY())),
12 - (MONTH(xcdhcm__DOB__c) - MONTH(TODAY())))
)
)
)
) &amp; &quot; month(s)&quot;</formula>
