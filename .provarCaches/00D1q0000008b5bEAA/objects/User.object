    <validationRules>
        <fullName>xcdhcm__Change_In_Name</fullName>
        <active>true</active>
        <description>User&apos;s first and last names should be changed using Change Name button on Personal.</description>
        <errorConditionFormula>CONTAINS(Profile.Name,&apos;HRMS&apos;) &amp;&amp; NOT(xcdhcm__Allow_Name_Change__c) &amp;&amp; NOT(ISNEW()) &amp;&amp;  (PRIORVALUE( FirstName)&lt;&gt;FirstName ||  PRIORVALUE(  LastName )&lt;&gt;LastName )</errorConditionFormula>
        <errorMessage>Change names for HR users using the Change name button from personal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Only_Admins_can_change_User_record</fullName>
        <active>true</active>
        <description>Validation rule prevents Employees, Team Leaders and Managers from changing their own User record.Only HR Admins and Payroll Managers and Admins can do this</description>
        <errorConditionFormula>and( 
$Profile.Name  &lt;&gt;  &quot;HRMS Admin&quot;,
$Profile.Name  &lt;&gt;  &quot;System Administrator&quot;,
$Profile.Name  &lt;&gt;  &quot;HRMS System Admin&quot;, 
$Profile.Name &lt;&gt; &quot;HRMS Payroll Manager&quot;, 
$Profile.Name &lt;&gt; &quot;HRMS Payroll Administrator&quot;,
or( 
ISCHANGED( Alias ) , 
ISCHANGED( IsActive ) ,  
ISCHANGED( City ) , 
ISCHANGED( Country ) ,  
ISCHANGED( Email ) , 
ISCHANGED( xcdhcm__empDetail_Id__c ) , 
ISCHANGED( EmployeeNumber ) , 
ISCHANGED( xcdhcm__End_Date__c ) , 
ISCHANGED( EndDay ) , 
ISCHANGED( Extension ) , 
ISCHANGED( Fax ) , 
ISCHANGED( xcdhcm__Generate_new_password__c ) , 
ISCHANGED( xcdhcm__User__c ) , 
ISCHANGED( xcdhcm__Manager_detail__c ) , 
ISCHANGED( CommunityNickname ) , 
ISCHANGED( ProfileId ) , 
ISCHANGED( xcdhcm__Start_Date__c ) , 
ISCHANGED( State ) , 
ISCHANGED( StartDay ) , 
ISCHANGED( Street ) , 
ISCHANGED( Title ) , 
ISCHANGED( Id ) , 
ISCHANGED( Username ) , 
ISCHANGED( UserType ) , 
ISCHANGED( CompanyName ), 
ISCHANGED( Department ), 
ISCHANGED( Division ), 
ISCHANGED( Email ), 
ISCHANGED( xcdhcm__empDetail_Id__c ),
/* NPPD-1773
ISCHANGED( FirstName ), 
ISCHANGED( LastName ), */
ISCHANGED( xcdhcm__Manager_detail__c ), 
ISCHANGED( CommunityNickname ), 
ISCHANGED( Phone ), 
ISCHANGED( Title )) 

)</errorConditionFormula>
        <errorMessage>You may change your mobile phone number but other fields may only be changed by Administrators. If you would like to change a value please contact one of the HR team</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Only_SystemAdmin_can_create_admin_record</fullName>
        <active>true</active>
        <description>Prevent admin profile from processing joiner for HRMS Admin or  HRMS system admin</description>
        <errorConditionFormula>AND
(
OR
(
$User.xcdhcm__UserRole__c = &apos;HRMS Admin&apos;,
$User.xcdhcm__UserRole__c = &apos;HRMS Payroll Administrator&apos;,
$User.xcdhcm__UserRole__c = &apos;HRMS Recruitment Administrator&apos;,
$User.xcdhcm__UserRole__c = &apos;HRMS Training Adminstrator&apos;,
$User.xcdhcm__UserRole__c = &apos;HRMS Trial Admin&apos;
),
OR
(
xcdhcm__UserRole__c = &apos;HRMS Admin&apos;,
xcdhcm__UserRole__c = &apos;HRMS Payroll Administrator&apos;,
xcdhcm__UserRole__c = &apos;HRMS Recruitment Administratorr&apos;,
xcdhcm__UserRole__c = &apos;HRMS System Admin&apos;,
xcdhcm__UserRole__c = &apos;HRMS Training Adminstrator&apos;,
xcdhcm__UserRole__c = &apos;HRMS Trial Admin&apos;,
xcdhcm__UserRole__c = &apos;System Administrator&apos;
)
)</errorConditionFormula>
        <errorMessage>Only HRMS System Administrators or System Administrators may create a user with an Administrator profile</errorMessage>
    </validationRules>
