    <validationRules>
        <fullName>xcdhcm__Change_In_Name</fullName>
        <active>true</active>
        <description>change name only from the change name button under personal tab of employee.</description>
        <errorConditionFormula>IF
(
  AND
  (
    CONTAINS( xcdhcm__User__r.Profile.Name , &apos;HRMS&apos;), 
    ISCHANGED(Name)
  ),
  IF
  (
	AND
	(
	    xcdhcm__User__r.xcdhcm__Allow_Name_Change__c, 
	    Name == xcdhcm__User__r.FirstName + &apos; &apos;+  xcdhcm__User__r.LastName
	),
	false,
	true
  ),
  false
)</errorConditionFormula>
        <errorMessage>Change names for HR users using the Change name button from personal.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Continuous_Service_Date</fullName>
        <active>true</active>
        <description>Do not deactivate</description>
        <errorConditionFormula>xcdhcm__Continuous_Service_Date__c &gt; xcdhcm__Start_Date__c</errorConditionFormula>
        <errorDisplayField>xcdhcm__Continuous_Service_Date__c</errorDisplayField>
        <errorMessage>Continuous Service date cannot be later than start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__EmpDetail_Sort_Code_Validation</fullName>
        <active>false</active>
        <description>Deprecated</description>
        <errorConditionFormula>IF(not(ISBLANK(Sort_Code__c)),NOT(REGEX(Sort_Code__c,&quot;[0-9]{2}-[0-9]{2}-[0-9]{2}&quot;)),FALSE)</errorConditionFormula>
        <errorMessage>Deprecated - Sort Code should be of the format 99-99-99</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Leaving_Date_not_before_Last_Working_Day</fullName>
        <active>true</active>
        <description>Leaving date cannot be earlier than Last working day</description>
        <errorConditionFormula>xcdhcm__Leaving_Date__c  &lt;  xcdhcm__Last_working_day__c</errorConditionFormula>
        <errorMessage>Leaving date cannot be earlier than Last working day.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_end_change_after_Payroll_cutoff</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(  xcdhcm__Payroll_end_date__c ),TODAY() &gt; DATE(YEAR(xcdhcm__Payroll_end_date__c ), MONTH(xcdhcm__Payroll_end_date__c ),VALUE(TEXT(xcdhcm__Company__r.xcdhcm__Payroll_cutoff_day__c))))</errorConditionFormula>
        <errorMessage>xx deprecated.  Payroll processing has started for this month. Please contact the Payroll Administrator immediately.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_end_date_not_before_Leaving_date</fullName>
        <active>true</active>
        <description>Do not deactivate - Payroll</description>
        <errorConditionFormula>and(
not(isnull( xcdhcm__Payroll_end_date__c )),
xcdhcm__Payroll_end_date__c &lt; xcdhcm__Leaving_Date__c )</errorConditionFormula>
        <errorMessage>Please set the Last pay run on or after the Employee’s Leaving date to ensure final payments are made correctly.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_start_after_Payroll_cutoff_day</fullName>
        <active>false</active>
        <description>xx deprecate</description>
        <errorConditionFormula>xcdhcm__Payroll_start_date__c &gt; DATE(YEAR(xcdhcm__Payroll_start_date__c), MONTH(xcdhcm__Payroll_start_date__c),VALUE(TEXT( xcdhcm__Company__r.xcdhcm__Payroll_cutoff_day__c)))</errorConditionFormula>
        <errorMessage>xx deprecatePayroll start date cannot be set for this month as processing is already under way. You must set the Payroll to start from the first of the following month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_start_change_after_Payrol_cutoff</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED( xcdhcm__Payroll_start_date__c),TODAY() &gt; DATE(YEAR(xcdhcm__Payroll_start_date__c), MONTH(xcdhcm__Payroll_start_date__c),VALUE(TEXT(xcdhcm__Company__r.xcdhcm__Payroll_cutoff_day__c))))</errorConditionFormula>
        <errorMessage>xx deprecatePayroll start date can not be changed as Payroll processing is under way. Please contact the Payroll Administrator immediately.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Payroll_start_not_before_current_month</fullName>
        <active>false</active>
        <description>Payroll start date can not be earlier than the first day of the month in which the Employee record is created</description>
        <errorConditionFormula>xcdhcm__Payroll_start_date__c &lt; DATE( YEAR( datevalue(CreatedDate) ), MONTH(datevalue(CreatedDate) ), 1)</errorConditionFormula>
        <errorDisplayField>xcdhcm__Payroll_start_date__c</errorDisplayField>
        <errorMessage>Payroll start date cannot be prior to current month</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>xcdhcm__Probationary_values_Rule</fullName>
